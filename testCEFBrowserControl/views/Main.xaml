        xmlns:cef="clr-namespace:CefGlue.Avalonia;assembly=CefGlue.Avalonia"
             >
    <DockPanel LastChildFill="True">
        <TextBlock DockPanel.Dock="Top">Above</TextBlock>

        <TextBlock DockPanel.Dock ="Bottom">Below</TextBlock>

        <cef:AvaloniaCefBrowser DockPanel.Dock="Top"  StartUrl="http://www.google.com" Name="browserCtrl" />
    </DockPanel>
</UserControl>